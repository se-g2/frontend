C;MAC7E,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACX,CAAC,CAAC,CAAC,CAAC,CAAC;IACL,CAAC,CAAC,CAAC,CAAC,CAAC","file":"C:/Users/18968/vue_demo/vue-demo/src/views/Verify.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <div style=\"width: 100%; height: 100vh; background-color: darkslateblue; overflow: hidden\">\r\n    <div style=\"width: 400px; margin: 100px auto\">\r\n        <div style=\"font-size: 30px; text-align: center; padding: 30px 0\">欢迎验证</div>\r\n      <el-form ref=\"form\" :model=\"form\" size=\"normal\" :rules=\"rules\">\r\n        <el-form-item prop=\"userno\">\r\n          <el-input prefix-icon=\"el-icon-user-solid\" v-model=\"form.userno\" placeholder=\"请输入学号\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"name\">\r\n          <el-input prefix-icon=\"el-icon-lock\" v-model=\"form.name\" placeholder=\"请输入姓名\"></el-input>\r\n        </el-form-item>\r\n        <el-button style=\"width: 100%\" type=\"primary\" @click=\"verify\">验证</el-button>    \r\n      </el-form>\r\n    </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\"\r\n\r\nexport default {\r\n    name:'Verify',\r\n    data(){\r\n        return {\r\n            form:{},\r\n            rules: {\r\n                userno: [\r\n                    {required: true, message: '请输入学工号', trigger: 'blur'},\r\n                ],\r\n                name: [\r\n                    {required: true, message: '请输入姓名', trigger: 'blur'},\r\n                ],\r\n            },\r\n        }\r\n    },\r\n    methods:{\r\n        verify(){\r\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form))  // 缓存用户信息\r\n          this.$router.push({path:'/',query: {userno: this.form.userno}})  //登录成功之后进行页面的跳转，跳转到主页\r\n          this.$refs['form'].validate((valid) => {\r\n              if (valid) {\r\n                request.post(\"/verify\", this.form).then(res => {\r\n                if (res.ok) {//\r\n                  this.$message({\r\n                    type: \"success\",\r\n                    message: \"验证成功\"\r\n                })\r\n                sessionStorage.setItem(\"user\", JSON.stringify(this.form))  // 缓存用户信息\r\n                this.$router.push({path:'/',query: {userno: this.form.userno}})  //登录成功之后进行页面的跳转，跳转到主页\r\n                } else {//此处应该对应于各种出错码\r\n                  this.$message({\r\n                  type: \"error\",\r\n                  message: res.msg\r\n                  })\r\n                }\r\n               })\r\n              }\r\n              else{\r\n                alert('submit!');\r\n              }\r\n            })\r\n          },\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n\r\n"]}]}